package javaapplication112;

import java.util.Scanner;

public class JavaApplication112 {

    public static void main(String[] args) {

        Scanner ler = new Scanner(System.in);
        //Variáveis
        int soma, cont;
        //Entrada de dados
        soma = 0;
        //Processamento
        for (cont = 1; cont <= 200; cont++) {
            if (cont % 2 != 0) {
                soma += cont;
            }
        }
        //Saida de dados
        System.out.println("A soma dos números ímpares de 1 a 200 é: " + soma);

    }

}
